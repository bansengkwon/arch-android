<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="stfStatefulLayout">
        <attr name="stfAnimationEnabled" format="boolean" />
        <attr name="stfInAnimation" format="reference" />
        <attr name="stfOutAnimation" format="reference" />
        <attr name="stfBackgroundColor" format="reference|color" />
        <attr name="stfColorMode">
            <flag name="white" value="0x10" />
            <flag name="black" value="0x20" />
        </attr>
    </declare-styleable>
</resources>